{
  "name": "synclan",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tauri dev",
    "build": "tauri build",
    "build:verbose": "tauri build --verbose",
    "tauri": "tauri",
    "app:dev": "pnpm web:build && pnpm ui:dev",
    "app:build": "pnpm web:build && pnpm ui:build",
    "app:preview": "pnpm web:preview && pnpm ui:preview",
    "ui:dev": "vite --config vite.config.main.ts",
    "ui:build": "tsc && vite build --config vite.config.main.ts",
    "ui:preview": "vite preview --config vite.config.main.ts",
    "web:dev": "vite --config vite.config.web.ts",
    "web:build": "tsc && vite build --config vite.config.web.ts",
    "web:preview": "vite preview --config vite.config.web.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "format:check": "prettier --check src --cache",
    "format:write": "prettier --write src --cache",
    "commitlint": "commitlint --edit",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-aria": "^3.41.1",
    "react-dom": "^19.1.0",
    "react-resizable-panels": "^3.0.3",
    "react-router": "^7.6.2",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@tailwindcss/vite": "^4.1.10",
    "@tauri-apps/cli": "^2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/browser": "^3.2.4",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.10",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^0.3.0"
  },
  "packageManager": "pnpm@10.12.1"
}
